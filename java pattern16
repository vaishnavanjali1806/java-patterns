package anjali;

public class datatypeproject1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		byte a= 10;
		short b= 20;
		int c=30;
		long d= 4000000000L;
		float e=   10.7f;
		char f= 'a';
		char ch = 99;// in b/w range 64000
		System.out.println(ch);
		boolean bit= false;
		if(bit) {
			System.out.println("hi");
		}else {
			System.out.println(" bye");
		}
		int g=32;
		System.out.println(g);

	
